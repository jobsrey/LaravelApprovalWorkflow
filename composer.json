{
  "name": "asetkita/laravel-approval-workflow",
  "description": "A flexible and powerful approval workflow system for Laravel 12+ applications with Spatie Media Library support",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "laravel-12", "approval", "workflow", "php", "spatie-medialibrary"],
  "authors": [
    {
      "name": "Rey",
      "email": "kireniusdena@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0|^12.0",
    "spatie/laravel-medialibrary": "^11.0|^12.0",
    "symfony/expression-language": "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "orchestra/testbench": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "AsetKita\\LaravelApprovalWorkflow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AsetKita\\LaravelApprovalWorkflow\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AsetKita\\LaravelApprovalWorkflow\\ApprovalWorkflowServiceProvider"
      ],
      "aliases": {
        "ApprovalWorkflow": "AsetKita\\LaravelApprovalWorkflow\\Facades\\ApprovalWorkflow"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
